# AI Provider Configuration
# Set AI_PROVIDER to 'claude' or 'openai' (defaults to claude if ANTHROPIC_API_KEY is set)
AI_PROVIDER=claude

# Anthropic (Claude) API Configuration - Used for Snap & Solve
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI API Configuration - Fallback/alternative provider
OPENAI_API_KEY=your_openai_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Feedback Feature
ENABLE_FEEDBACK_FEATURE=true
